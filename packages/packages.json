            "tar -xf $CACHE_PATH/$app_name/vmware-tools-windows-12.4.5-23787635.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-windows-x86-12.4.5-23787635.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-winPreVista-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-linux-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-linuxPreGlibc25-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-netware-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-solaris-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "tar -xf $CACHE_PATH/$app_name/vmware-tools-winPre2k-12.4.0-23259341.x86_64.component.tar -C $CACHE_PATH/$app_name",
            "sudo systemctl mask vmware.service",
            "sudo chmod +x $CACHE_PATH/$app_name/VMware-Workstation-17.6.2-24409262.x86_64.bundle",
            "sudo $CACHE_PATH/$app_name/VMware-Workstation-17.6.2-24409262.x86_64.bundle --console --required --eulas-agreed --install-component=vmware-tools-windows-12.4.5-23787635.x86_64.component --install-component=vmware-tools-windows-x86-12.4.5-23787635.x86_64.component --install-component=vmware-tools-winPreVista-12.4.0-23259341.x86_64.component --install-component=vmware-tools-linux-12.4.0-23259341.x86_64.component --install-component=vmware-tools-linuxPreGlibc25-12.4.0-23259341.x86_64.component --install-component=vmware-tools-netware-12.4.0-23259341.x86_64.component --install-component=vmware-tools-solaris-12.4.0-23259341.x86_64.component --install-component=vmware-tools-winPre2k-12.4.0-23259341.x86_64.component",
            "sudo systemctl unmask vmware.service",
            "echo \"Starting the DKMS build process.\"",
            "sudo mkdir -p /usr/src/$dkms_name-$version",
            "sudo tar -xf /usr/lib/vmware/modules/source/vmmon.tar -C /usr/src/$dkms_name-$version",
            "sudo tar -xf /usr/lib/vmware/modules/source/vmnet.tar -C /usr/src/$dkms_name-$version",
            "sudo wget https://aur.archlinux.org/cgit/aur.git/plain/Makefile?h=vmware-workstation -O /usr/src/$dkms_name-$version/Makefile",
            "sudo wget https://aur.archlinux.org/cgit/aur.git/plain/dkms.conf.in?h=vmware-workstation -O /usr/src/$dkms_name-$version/dkms.conf",
            "sudo sed -i 's/@PKGNAME@/$dkms_name/' /usr/src/$dkms_name-$version/dkms.conf",
            "sudo sed -i 's/@PKGVER@/$version/' /usr/src/$dkms_name-$version/dkms.conf",
            "wget https://aur.archlinux.org/cgit/aur.git/plain/vmmon.patch?h=vmware-workstation -O $CACHE_PATH/$app_name/vmmon.patch",
            "wget https://aur.archlinux.org/cgit/aur.git/plain/vmnet.patch?h=vmware-workstation -O $CACHE_PATH/$app_name/vmnet.patch",
            "sudo patch -N -p2 -d /usr/src/$dkms_name-$version/vmmon-only < $CACHE_PATH/$app_name/vmmon.patch",
            "sudo patch -N -p2 -d /usr/src/$dkms_name-$version/vmnet-only < $CACHE_PATH/$app_name/vmnet.patch",
            "sudo dkms add --force -m $dkms_name -v $version",
            "sudo dkms build --force -m $dkms_name -v $version",
            "sudo dkms install --force -m $dkms_name -v $version",
            "sudo vmware-modconfig --console --install-all"
